    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Exporters Manager",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-manager",
            "program": "${workspaceFolder}/Build/Debug/net5.0/EventLogExportersManager.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Build/Debug/net5.0",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Exporter",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-exporter",
            "program": "${workspaceFolder}/Build/Debug/net5.0/EventLogExporter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Build/Debug/net5.0",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}